{
  "name": "sms-management-api",
  "version": "1.0.0",
  "description": "An sms management app",
  "main": "index.js",
  "scripts": {
    "migrate": "sequelize db:migrate:undo:all && sequelize db:migrate",
    "start": "npm run build && node dist/index.js",
    "build": "rimraf dist/ && babel ./src --out-dir dist/ --copy-files",
    "start:dev": "nodemon --watch server --exec babel-node ./index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/d-beloved/sms-management-API.git"
  },
  "keywords": [
    "NODE",
    "EXPRESS",
    "POSTGRES"
  ],
  "engines": {
    "node": "12.4.0",
    "npm": "6.9.0"
  },
  "author": "d-beloved",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/d-beloved/sms-management-API/issues"
  },
  "homepage": "https://github.com/d-beloved/sms-management-API#readme",
  "dependencies": {
    "body-parser": "^1.20.1",
    "dotenv": "^8.1.0",
    "express": "^4.18.2",
    "fancy-log": "^1.3.3",
    "morgan": "^1.9.1",
    "pg": "^7.12.1",
    "pg-hstore": "^2.3.3",
    "rimraf": "^3.0.0",
    "sequelize": "^5.16.0",
    "sequelize-cli": "^5.5.0",
    "validator": "^11.1.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.5.5",
    "@babel/node": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "babel-eslint": "^10.0.2",
    "eslint": "^6.2.2",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-plugin-import": "^2.18.2",
    "nodemon": "^1.19.1"
  }
}
